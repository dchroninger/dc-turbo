{
  "name": "ui",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/styles.css"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react && tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "dev": "concurrently \"tsup src/index.tsx --format esm,cjs --dts --external react --watch\" \"tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch\"",
    "test": "jest --coverage",
    "clean": "rm -rf dist",
    "storybook": "concurrently \"start-storybook -p 6006\" \"tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch\"",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@jest/globals": "^29.4.1",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/cli": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "babel-loader": "^8.3.0",
    "class-variance-authority": "^0.4.0",
    "concurrently": "^7.2.2",
    "eslint": "^7.32.0",
    "eslint-config-custom": "workspace:*",
    "jest-config": "workspace:*",
    "react": "^18.2.0",
    "storybook-tailwind-dark-mode": "^1.0.15",
    "tailwind-config": "workspace:*",
    "tailwindcss": "^3.2.4",
    "tsconfig": "workspace:*",
    "tsup": "^6.1.3",
    "typescript": "^4.9.4",
    "@testing-library/react": "^13.4.0"
  }
}
